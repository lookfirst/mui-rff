{
  "name": "mui-rff",
  "version": "5.3.2",
  "license": "MIT",
  "description": "Set of modern wrapper components to facilitate using Material UI with React Final Form",
  "repository": {
    "url": "https://github.com/lookfirst/mui-rff.git",
    "type": "git"
  },
  "homepage": "https://lookfirst.github.io/mui-rff",
  "keywords": [
    "react",
    "material-ui",
    "final-form",
    "react-final-form",
    "yup"
  ],
  "author": "Jon Stevens",
  "main": "dist/index.js",
  "module": "dist/mui-rff.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "jest",
    "testw": "dts test --watch",
    "lint": "dts lint src test --ignore-pattern node_modules",
    "lint-fix": "yarn lint --fix",
    "ui": "yarn upgradeInteractive; cd example; yarn ui; yarn upgrade",
    "predeploy": "cd example; yarn; yarn build",
    "deploy": "gh-pages -d example/dist",
    "prepublish": "yarn build",
    "postpublish": "yarn deploy",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.5",
    "@mui/styles": "^5.10.3",
    "@mui/system": "^5.10.5",
    "@mui/x-date-pickers": "^5.0.1",
    "final-form": ">=4.19.1 < 5",
    "react": ">=18",
    "react-final-form": "^6.5.9"
  },
  "optionalDependencies": {
    "@date-io/core": "^2.11.0",
    "@date-io/date-fns": "^2.11.0",
    "date-fns": "^2.25.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.10.5",
    "@mui/styles": "^5.10.3",
    "@mui/system": "^5.10.5",
    "@mui/x-date-pickers": "^5.0.1",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "dts-cli": "^1.5.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "final-form": "^4.20.6",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-summary-reporter": "^0.0.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "ts-jest": "^29.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  }
}
